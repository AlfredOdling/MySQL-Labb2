{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://AlfredOdling.github.io/react-boilerplate-2-simple",
  "dependencies": {
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-scripts": "^1.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "sass": "sass --watch src/styles/scss:src/styles/css",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0"
  },
  "prettier": {
    "eslint.enable": true,
    "files.exclude": {
      "node_modules": true,
      ".npm-offline-cache": true
    },
    "editor.formatOnSave": true,
    "[javascriptreact]": {
      "editor.formatOnSave": true
    },
    "singleQuote": true,
    "semi": false
  }
}
